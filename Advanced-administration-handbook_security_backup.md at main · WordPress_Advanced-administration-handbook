.ToggleSwitch-track:focus, .ToggleSwitch-track:focus-visible { outline-offset: 1px; }

.ToggleSwitch-track:hover { background-color: var(--controlTrack-bgColor-hover, var(--color-switch-track-hover-bg)); }

